#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--)
	{
	    int N;
	    cin>>N;
	    if(N>=1)
	    {
	        if(N % 3 == 0)
	            cout<<"Yes"<<endl;
	        else
	            cout<<"No"<<endl;
	    }
	    else
	        break;
	}
	return 0;
}
